{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "Launch: cumulative-avg",
      "command": "node --require source-map-support/register dist/cumulative-avg.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "tsc watch",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}
